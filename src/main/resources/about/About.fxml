<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXMasonryPane?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <BorderPane minHeight="640" minWidth="840">
        <stylesheets>
            <URL value="@../base.css"/>
        </stylesheets>
        <center>
            <JFXMasonryPane minWidth="640">
                <VBox styleClass="card">
                    <StackPane styleClass="header" VBox.vgrow="ALWAYS">
                        <Label fx:id="aboutTheoryHeader"/>
                    </StackPane>
                    <StackPane prefHeight="100.0" prefWidth="680.0" styleClass="body">
                        <JFXTextArea fx:id="aboutTheoryTextArea"/>
                    </StackPane>
                </VBox>
                <VBox styleClass="card">
                    <StackPane styleClass="header" VBox.vgrow="ALWAYS">
                        <Label fx:id="aboutModelHeader"/>
                    </StackPane>
                    <StackPane prefHeight="550.0" prefWidth="680.0" styleClass="body">
                        <JFXTextArea fx:id="aboutModelTextArea"/>
                    </StackPane>
                </VBox>
                <BorderPane.margin>
                    <Insets left="120.0" top="10.0" bottom="-10"/>
                </BorderPane.margin>
            </JFXMasonryPane>
        </center>
    </BorderPane>
</AnchorPane>